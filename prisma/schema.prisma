datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Transaction {
    id              Int      @id @default(autoincrement())
    buyer           String
    seller          String
    houseNumber     String
    surveyNumber    String
    documentNumber  String
    transactionDate DateTime
    value           Float
    originalText    String?
    createdAt       DateTime @default(now())
}
